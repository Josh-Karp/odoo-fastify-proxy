{
  "name": "@odoo-fastify/proxy",
  "version": "0.0.1",
  "description": "Odoo API proxy server built with Fastify",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js",
    "info": "fastify print-routes ./routes/root.js"
  },
  "keywords": [],
  "author": "Joshua Karp <joshua@sitelab.co.za>",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/env": "^4.2.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.3.1",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-env": "^2.2.0",
    "fastify-plugin": "^4.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "tap": "^16.1.0"
  }
}
