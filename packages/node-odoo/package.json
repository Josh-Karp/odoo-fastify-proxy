  "bugs": {
    "url": "https://github.com/Josh-Karp/node-odoo/issues"
  },
  "homepage": "https://github.com/Josh-Karp/node-odoo#readme",
  "devDependencies": {
    "standard": "^17.0.0",
    "tap": "^16.1.0"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "uuid": "^9.0.0"
  },
  "publishConfig": {
